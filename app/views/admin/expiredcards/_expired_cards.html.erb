<table>
  <thead>
    <tr>
      <th>S.N</th>
      <th>Name</th>
      <th>Email</th>
      <th>Expired at</th>
      <th>Plan ID</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each_with_index do |user, index| %>
      <tr>
        <td><%= index+1 %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.expiration %></td>
        <td><%= user.subscriptions.try(:last).try(:plan_id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
